@font-face
	font-family: 'Din'
	src: url('fonts/Din/subset-DINPro-Light.eot')
	src: url('fonts/Din/subset-DINPro-Light.eot?#iefix') format('embedded-opentype'),
		url('fonts/Din/subset-DINPro-Light.woff2') format('woff2'),
		url('fonts/Din/subset-DINPro-Light.woff') format('woff'),
		url('fonts/Din/subset-DINPro-Light.ttf') format('truetype')
	font-weight: 300
	font-style: normal

@font-face
	font-family: 'Din'
	src: url('fonts/Din/DINPro-Bold.eot')
	src: url('fonts/Din/DINPro-Bold.eot?#iefix') format('embedded-opentype'),
		url('fonts/Din/DINPro-Bold.woff2') format('woff2'),
		url('fonts/Din/DINPro-Bold.woff') format('woff'),
		url('fonts/Din/DINPro-Bold.ttf') format('truetype')
	font-weight: normal
	font-style: normal
